<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="500" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <!-- Fondo de ventana -->
  <rect width="800" height="500" fill="#f8f9fa"/>
  
  <!-- Barra de título -->
  <rect width="800" height="40" fill="#d4edda"/>
  <text x="20" y="25" fill="#155724" font-family="Arial" font-size="14" font-weight="bold">Texto Procesado - Después del Preprocesamiento</text>
  
  <!-- Contenido del texto -->
  <rect x="20" y="60" width="760" height="420" fill="#ffffff" stroke="#c3e6cb" stroke-width="2"/>
  
  <text x="40" y="100" fill="#155724" font-family="Arial" font-size="16" font-weight="bold">Resultado del Preprocesamiento:</text>
  
  <text x="40" y="140" fill="#495057" font-family="Arial" font-size="14">Tokens limpios obtenidos:</text>
  
  <!-- Lista de tokens procesados -->
  <rect x="40" y="160" width="720" height="200" fill="#f8f9fa" stroke="#e9ecef"/>
  
  <text x="60" y="185" fill="#28a745" font-family="Consolas, monospace" font-size="13">['hola', 'mundo', 'ejemplo', 'texto', 'contiene', 'mayusculas',</text>
  <text x="60" y="205" fill="#28a745" font-family="Consolas, monospace" font-size="13">'minusculas', 'mezcladas', 'signos', 'puntuacion', 'numeros',</text>
  <text x="60" y="225" fill="#28a745" font-family="Consolas, monospace" font-size="13">'palabras', 'vacias', 'stopwords', 'espacios', 'multiples',</text>
  <text x="60" y="245" fill="#28a745" font-family="Consolas, monospace" font-size="13">'palabras', 'procesamiento', 'lenguaje', 'natural', 'nlp',</text>
  <text x="60" y="265" fill="#28a745" font-family="Consolas, monospace" font-size="13">'rama', 'inteligencia', 'artificial', 'ocupa', 'interaccion',</text>
  <text x="60" y="285" fill="#28a745" font-family="Consolas, monospace" font-size="13">'computadoras', 'lenguaje', 'humano', 'fascinante',</text>
  <text x="60" y="305" fill="#28a745" font-family="Consolas, monospace" font-size="13">'palabras', 'acentos', 'analisis', 'programacion',</text>
  <text x="60" y="325" fill="#28a745" font-family="Consolas, monospace" font-size="13">'linguistica', 'incluye', 'contracciones']</text>
  
  <!-- Transformaciones aplicadas -->
  <text x="40" y="390" fill="#495057" font-family="Arial" font-size="14" font-weight="bold">Transformaciones aplicadas:</text>
  <text x="60" y="415" fill="#6c757d" font-family="Arial" font-size="12">✓ Conversión a minúsculas</text>
  <text x="60" y="430" fill="#6c757d" font-family="Arial" font-size="12">✓ Eliminación de signos de puntuación</text>
  <text x="60" y="445" fill="#6c757d" font-family="Arial" font-size="12">✓ Eliminación de stopwords</text>
  <text x="60" y="460" fill="#6c757d" font-family="Arial" font-size="12">✓ Filtrado de tokens alfabéticos únicamente</text>
  
  <!-- Estadísticas después -->
  <rect x="600" y="80" width="180" height="120" fill="#d4edda" stroke="#c3e6cb"/>
  <text x="610" y="100" fill="#155724" font-family="Arial" font-size="12" font-weight="bold">Estadísticas finales:</text>
  <text x="610" y="120" fill="#155724" font-family="Arial" font-size="11">Tokens válidos: 32</text>
  <text x="610" y="135" fill="#155724" font-family="Arial" font-size="11">Reducción: 64%</text>
  <text x="610" y="150" fill="#155724" font-family="Arial" font-size="11">Solo minúsculas: ✓</text>
  <text x="610" y="165" fill="#155724" font-family="Arial" font-size="11">Sin puntuación: ✓</text>
  <text x="610" y="180" fill="#155724" font-family="Arial" font-size="11">Sin stopwords: ✓</text>
  
  <!-- Comparación visual -->
  <text x="300" y="390" fill="#495057" font-family="Arial" font-size="12">89 palabras → 32 tokens limpios</text>
  <path d="M 350 395 L 380 395" stroke="#28a745" stroke-width="2" marker-end="url(#arrow)"/>
  
  <defs>
    <marker id="arrow" markerWidth="10" markerHeight="10" refX="9" refY="3" orient="auto" markerUnits="strokeWidth">
      <path d="M0,0 L0,6 L9,3 z" fill="#28a745"/>
    </marker>
  </defs>
</svg>